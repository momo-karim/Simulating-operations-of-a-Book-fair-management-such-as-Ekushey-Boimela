        <Label layoutX="14.0" layoutY="14.0" text="Generate Reports" style="-fx-font-size: 20px;" />
        <TableView fx:id="reportsTable" layoutX="14.0" layoutY="50.0" prefHeight="200.0" prefWidth="572.0">
